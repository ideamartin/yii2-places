{
  "name": "nevermnd/yii2-places",
  "description": "Yii2 typeahead google places autocomplete widget",
  "minimum-stability": "dev",
  "license": "MIT",
  "keywords": [
    "yii2",
    "google",
    "places",
    "autocomplete",
    "typeahead"
  ],
  "support": {
    "source": "https://github.com/nevermnd/yii2-places"
  },
  "authors": [
    {
      "name": "Skiptir Engu",
      "email": "skiptir.engu@yandex.com"
    }
  ],
  "extra": {
    "asset-repositories": [
      {
        "type": "bower-github",
        "url": "https://github.com/sgruhier/typeahead-addresspicker"
      }
    ]
  },
  "require": {
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "bower-asset/typeahead.js": "*",
    "bower-asset/typeahead-addresspicker": "dev-typeahead-0.11.1 as 0.1.4",
    "yiisoft/yii2-gii": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "nevermnd\\places\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "nevermnd\\places\\tests\\": "tests/"
    }
  }
}
